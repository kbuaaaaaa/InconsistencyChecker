{
    "env": {
        "browser": true,
        "es2021": true, 
        "webextensions": true,
        "jquery": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-use-before-define": "off",
        "no-undef": "off", 
        "func-names": "off", 
        "no-restricted-syntax": "off", 
        "guard-for-in": "off"
    }
}
