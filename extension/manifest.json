{
  "name" : "Identify UI widgets",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "A small tool that can identify the same site to see different pages.",
  "author": "se04",
  "icons": {
    "16": "img/1.png"

  },
  "content_scripts": [
    {"matches": ["<all_urls>"],"js": ["index.js"]}
  ],
  "browser_action": {
    "default_icon":"img/1.png",
    "default_popup": "popup.html"
  }
}